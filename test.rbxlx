          local tblA = { valA1 = 1 }
          local tblB = { valB1 = 2 }
          local result = augment(tblA, nil, 5, tblB)
          expect(result.valB1).to.equal(2)
        end)
        it("should not affect #args[1]", function()
          local tblA = { 1, 2, 3, 4 }
          local tblB = { valB1 = 2 }
          local result = augment(tblA, tblB)
          expect(#result).to.equal(4)
        end)
      end)
    end)
  end)
